=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#fafafa\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"mbr-section-head\" mbr-if=\"showTitle || showSubtitle\">\n            <h4 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><b>Checklist</b></h4>\n            <h5 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-0 mt-2\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">Images with Title and Text</h5>\n        </div>\n        <div class=\"row mt-5\">\n            \n            <div class=\"item features-image сol-12 col-md-6 col-lg-{{cardsWidth}} active\">\n                <div class=\"item-wrapper threesCard\">\n                    <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle || showText || showButtons\">\n                        <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\"><b>Rice and Beans</b></h5>\n                          <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/432142-1631204385-400x400.jpg\" alt title data-slide-to=\"3\" data-bs-slide-to=\"3\">\n                    </div>\n                        <div class=\"item-subtitle mbr-fonts-style mt-1\" mbr-if=\"showCardSubtitle\"><span class=\"mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".item-subtitle\" mbr-text>₦1600</span>\n                            <span class=\"mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-subtitle\" mbr-text>&nbsp;</span>\n                         </div>\n                        <p class=\"mbr-text mbr-fonts-style mt-3\" mbr-if=\"showText\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-text, .mbr-section-btn\">LOREM IPSUM DOLOR\n<br>SIT AMET CONSECT\n<br>ETUER ADIPISCING ELIT</p>\n                    </div>\n                    <div class=\"mbr-section-btn item-footer mt-2\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-buttons mbr-theme-style=\"display-4\"><a href=\"https://api.whatsapp.com/send?phone=2347067874052&text=Please%20i%20need%20Rice%20and%20Beans%20%E2%82%A61500\" class=\"btn btn-lg item-btn btn-info\" target=\"_blank\" data-app-placeholder=\"Type Text\">Order Now</a></div>\n                </div>\n            </div><div class=\"item features-image сol-12 col-md-6 col-lg-{{cardsWidth}}\">\n                <div class=\"item-wrapper firstCard\">\n                  \n                    <div class=\"item-content\" mbr-if=\"showCardTitle || showCardSubtitle || showText || showButtons\">\n                        <h5 class=\"item-title mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".item-title\" mbr-if=\"showCardTitle\"><b>Fired Rice</b></h5>\n                          <div class=\"item-img\">\n                        <img src=\"@PROJECT_PATH@/assets/images/432141-1631204386-400x400.jpg\" alt title data-slide-to=\"0\" data-bs-slide-to=\"0\">\n                    </div>\n                        <div class=\"item-subtitle mbr-fonts-style mt-1\" mbr-if=\"showCardSubtitle\"><span class=\"mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".item-subtitle\" mbr-text>₦1600</span>\n                            <span class=\"mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-subtitle\" mbr-text>&nbsp;</span>\n        